{
  "name": "flashcard-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cd client && vite",
    "server": "cd server && tsx src/server.ts",
    "start": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "cd client && tsc -b && vite build",
    "render-build": "npm ci && npm run build",
    "render-start": "cd server && node --experimental-modules src/server.js",
    "lint": "cd client && eslint .",
    "preview": "cd client && vite preview",
    "check-db": "node --experimental-json-modules scripts/database/check-db.js",
    "setup-db": "node scripts/database/setup-db.js"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@sentry/node": "^10.12.0",
    "@sentry/react": "^10.17.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/multer": "^1.4.13",
    "ajv-keywords": "^3.5.2",
    "antd": "^5.24.8",
    "axios": "^1.8.4",
    "canvas": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "multer": "^2.0.1",
    "pdf.js-extract": "^0.2.1",
    "pdf2json": "^3.1.4",
    "pdfjs-dist": "^4.8.69",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.30.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
